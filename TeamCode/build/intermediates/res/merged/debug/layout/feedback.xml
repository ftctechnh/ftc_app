<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/feedback"
              xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_margin="5dip"
              android:background="?attr/feedbackShape"
              >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="1dip"
        android:padding="1dip"
        android:gravity="center"
        android:orientation="vertical"
        >

        <TextView style="@style/FeedbackText"
            android:id="@+id/feedbackText0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="3dip"
            android:text="(replaced)"/>

        <TextView style="@style/FeedbackText"
            android:id="@+id/feedbackText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="3dip"
            android:text="(replaced)"/>

        <Button style="@style/FeedbackButton"
            android:id="@+id/feedbackOKButton"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/buttonNameOK"/>

    </LinearLayout>
</LinearLayout>
